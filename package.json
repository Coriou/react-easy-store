{
	"name": "@coriou/react-easy-store",
	"version": "0.0.8",
	"main": "dist/main.js",
	"repository": "Coriou/react-easy-store.git",
	"author": "Benjamin Coriou",
	"license": "MIT",
	"private": false,
	"browserslist": "> 0.25%, not dead, IE 9",
	"scripts": {
		"dev": "parcel ./examples/index.html",
		"build": "make-dir dist && rimraf dist/* && yarn babel src/index.js --out-file dist/main.js --presets=@babel/env,@babel/react,minify --source-maps --no-comments",
		"analyze": "yarn build && bundlesize -f 'dist/*.js'",
		"deploy": "yarn analyze && yarn publish --access public",
		"build:example": "parcel build ./examples/index.html --out-dir build --public-url https://coriou.github.io/react-easy-store/",
		"lint": "eslint --ext .js src"
	},
	"peerDependencies": {
		"mobx": ">=4",
		"react": ">=16.8.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@babel/preset-react": "^7.9.4",
		"babel-eslint": "^10.1.0",
		"babel-preset-minify": "^0.5.1",
		"bundlesize": "^0.18.0",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"make-dir-cli": "^2.0.0",
		"mobx": "^5.15.4",
		"parcel-bundler": "^1.12.4",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"rimraf": "^3.0.2",
		"source-map-explorer": "^2.4.2",
		"yarn": "^1.22.5"
	},
	"dependencies": {}
}
